{
  "name": "devbox",
  "version": "0.5.1-beta",
  "type": "module",
  "bin": {
    "devbox": "./bin/devbox"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build ./src/index.ts --compile --outfile=devbox-bin",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "check": "biome check --write ./src",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "chalk": "5.6.2",
    "commander": "14.0.2",
    "execa": "9.6.1",
    "inquirer": "13.2.1",
    "ora": "9.1.0",
    "tar": "7.5.6",
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@types/bun": "1.3.6",
    "@types/inquirer": "9.0.9",
    "@types/tar": "6.1.13",
    "vitepress": "1.6.4"
  }
}
